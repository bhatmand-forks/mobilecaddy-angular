    "core-js",
    "rxjs",
    "debug",
    "zone.js",
    "ionic",
    "ionic-native",
    "sw-toolbox",
    "mobilecaddy-utils",
    "mobilecaddy",
    "underscore",
    "@ngx-translate/core"
  ]
}
